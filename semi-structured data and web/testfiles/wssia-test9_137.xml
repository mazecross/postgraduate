<!DOCTYPE expression PUBLIC "calc1" "http://www.cs.man.ac.uk/~sattler/teaching/calc1.dtd">
<!--(3-(7-2))+(10+3+(2*((5+3)-1)*9))-->
<expression>
    <plus>
        <minus>
            <int value="3"/>
            <minus>
                <int value="7"/>
                <int value="2"/>
            </minus>
        </minus>
		<plus>
			<int value="10"/>
			<int value="3"/>
			<times>
				<int value="2"/>
				<minus>
					<!--<int value="8"/>-->
					<plus>
						<int value="5"/>
						<int value="3"/>
					</plus>
					<int value="1"/>
				</minus>
				<int value="9"/>
			</times>
		</plus>
    </plus>
</expression>
